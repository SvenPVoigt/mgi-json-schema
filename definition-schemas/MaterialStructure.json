{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "title": "MaterialStructure",
    "definitions": {"MaterialStructure": {
        "type": "object",
        "properties": {
            "lengthScale": {
                "title": "Structure Length Scale",
                "description": "A qualitative description of the length scale at which the structure is defined. Could be atomic, microscale, mesoscale, etc. QUantitative measurements are given in dimensions.",
                "type": "string"
            },
            "dimensions": {
                "title": "Dimensions of Structure",
                "description": "Dimensions are applicable if there is a measurement taken over some defined length, area, or volume. Accepted is an array of Quantitative Values that need not necessarily be constrained to 3 dimensions (i.e. 3 spatial dimensions + 3 euler angles = 6 dimensions). Units of the Quantitative Value do not accept the axis. Dimensions matching the order of dimensions in attached payloads are prefered. See more about Quantitative Values at <href a=#/QuantitativeValue>.",
                "type": "array",
                "items": {
                    "title": "Dimension",
                    "$ref": "PropertyValue.json#/definitions/PropertyValue"
                }
            },
            "includedInDataCatalog": {"$ref": "includedInDataCatalog.json#/definitions/includedInDataCatalog"},
            "material": {
                "title": "Material",
                "$ref": "materialLinked.json#/definitions/materialLinked"
            },
            "materialExtent": {
                "title": "Material Quantity",
                "$ref": "QuantitativeValue.json#/definitions/QuantitativeValue"
            },
            "materialReference": {
                "title": "Reference Material",
                "$ref": "materialLinked.json#/definitions/materialLinked"
            },
            "materialReferenceExtent": {
                "title": "Reference Material Quantity",
                "$ref": "QuantitativeValue.json#/definitions/QuantitativeValue"
            },
            "process": {
                "title": "Reference Process",
                "$ref": "processLinked.json#/definitions/processLinked"
            },
            "measurementTechnique": {"$ref": "measurementTechnique.json#/definitions/measurementTechnique"},
            "variableMeasured": {
                "title": "Material Property",
                "$ref": "PropertyValue.json#/definitions/PropertyValue"
            },
            "parameterControlled": {
                "type": "array",
                "title": "Parameters Controlled",
                "uniqueItems": true,
                "items": {
                    "title": "Parameter Controlled",
                    "$ref": "PropertyValue.json#/definitions/PropertyValue"
                }
            },
            "conditionObserved": {
                "type": "array",
                "title": "Conditions Observed",
                "uniqueItems": true,
                "items": {
                    "title": "Condition Observed",
                    "$ref": "PropertyValue.json#/definitions/PropertyValue"
                }
            },
            "hasPart": {"$ref": "datasetLinked.json#/definitions/hasPart"},
            "isPartOf": {"$ref": "datasetLinked.json#/definitions/isPartOf"},
            "isBasedOn": {"$ref": "datasetLinked.json#/definitions/isBasedOn"}
        }
    }}
}